{
  "name": "seo-star",
  "displayName": "Seo Star",
  "version": "0.0.1",
  "description": "User-Friendly SEO Companion. This browser extension is your key to unlocking the world of search engine optimization.",
  "author": "nice@tiangong.dev",
  "scripts": {
