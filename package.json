    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "plasmo": "^0.89.5",
    "radash": "^12.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-json-view-lite": "^1.5.0",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
